{
  "name": "soild-pocketbase-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "pnpm -env-file=.env vite --host",
    "i": "pnpm install",
    "dev": "pnpm --env-file=.env vite --host",
    "build": "pnpm tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "pnpm vite preview",
    "typegen": "npx typed-pocketbase -d src/types -f auth,posts",
    "android": "npx cap run android",
    "sync": "npx cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/ios": "^7.2.0",
    "@corvu/resizable": "^0.2.4",
    "@formkit/auto-animate": "^0.8.2",
    "@kobalte/core": "^0.13.7",
    "@solid-primitives/autofocus": "^0.0.111",
    "@solid-primitives/mutation-observer": "^1.2.0",
    "@solid-primitives/refs": "^1.1.2",
    "@solid-primitives/scheduled": "^1.5.2",
    "@solidjs/router": "^0.15.3",
    "@soorria/solid-dropzone": "^1.0.1",
    "@tanstack/solid-query": "^5.90.3",
    "@tanstack/solid-store": "^0.6.0",
    "arktype": "^2.1.19",
    "classnames": "^2.5.1",
    "flag-icons": "^7.3.2",
    "pocketbase": "^0.25.2",
    "sass": "^1.81.0",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.3",
    "solid-outside-click-handler": "^0.1.0",
    "solid-toast": "^0.5.0",
    "solid-transition-group": "^0.3.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2"
  }
